<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="Pokedex.Resources.Styles.ColorsSetYellow">

    <Color x:Key="Primary">#C19E1D</Color>
    <Color x:Key="PrimaryDark">#B2B200</Color>
    <Color x:Key="PrimaryDarkText">#666600</Color>
    <Color x:Key="Secondary">#FCEBA6</Color>
    <Color x:Key="SecondaryDarkText">#CCCC33</Color>
    <Color x:Key="Tertiary">#7B2C1F</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>

</ResourceDictionary>