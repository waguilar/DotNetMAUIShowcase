<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="Pokedex.Resources.Styles.ColorsSetRed">

    <Color x:Key="Primary">#FF0000</Color>
    <Color x:Key="PrimaryDark">#B20000</Color>
    <Color x:Key="PrimaryDarkText">#660000</Color>
    <Color x:Key="Secondary">#FFBFBF</Color>
    <Color x:Key="SecondaryDarkText">#CC3333</Color>
    <Color x:Key="Tertiary">#990000</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>
    
</ResourceDictionary>