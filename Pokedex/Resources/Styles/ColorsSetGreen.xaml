<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="Pokedex.Resources.Styles.ColorsSetGreen">

    <Color x:Key="Primary">#44bb22</Color>
    <Color x:Key="PrimaryDark">#00B200</Color>
    <Color x:Key="PrimaryDarkText">#006600</Color>
    <Color x:Key="Secondary">#66FF66</Color>
    <Color x:Key="SecondaryDarkText">#33CC33</Color>
    <Color x:Key="Tertiary">#009900</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>

</ResourceDictionary>